{
  "name": "SamMiti",
  "platforms": {
    "ios": "11.0"
  },
  "version": "0.0.2",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://bitbucket.org/prolificinteractive/sammiti-ios",
  "authors": {
    "Nattawut Singhchai": "wut@prolificinteractive.com"
  },
  "summary": "An iOS framework for easy 3D thing integration to your application.",
  "source": {
    "git": "git@bitbucket.org:prolificinteractive/sammiti-ios.git",
    "tag": "0.0.2"
  },
  "source_files": "SamMiti/**/*.{swift,h}",
  "subspecs": [
    {
      "name": "AR",
      "source_files": "SamMitiAR/**/*.swift",
      "resources": [
        "SamMitiAR/**/*.{storyboard,xib,strings}",
        "SamMitiAR/SamMitiArt.scnassets",
        "SamMitiAR/SamMitiAssets.xcassets"
      ],
      "dependencies": {
        "GLTFSceneKit": [
          "~>0.1"
        ]
      }
    },
    {
      "name": "Scene",
      "source_files": "SamMitiScene/**/*.swift"
    }
  ]
}
